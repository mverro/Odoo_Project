<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="school_profile_report_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="external_layout">
                    <div class="page">
                        <h1>Hello How Are you ?</h1>
                    </div>
                </t>
            </t>
        </t>
    </template>

    <record id="school_profile_report_report_temp" model="ir.actions.report">
        <field name="name">Student Card</field>
        <field name="model">school.profile</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">qweb_pdf_report_example.school_profile_report_report_template</field>
        <field name="report_file">qweb_pdf_report_example.school_profile_report_report_template</field>
        <field name="print_report_name">'Student-Card'</field>
        <field name="binding_model_id" ref="school_profile.model_school_profile"></field>
        <field name="binding_type">report</field>
    </record>

</odoo>